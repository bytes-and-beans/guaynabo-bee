{# Handles the entries for Gobierno section, pulling in the appropriate layout 
   based on the entry type.
#}

{% switch entry.type %}
    {% case "comunidad" %}
        {% include "_layouts/content-and-sidebar" %}
    {% case "comunidadNoSidebar" %}
        {% include "_layouts/content-no-sidebar" %}
    {% case "comunidadHeader" %}
        {% include "_layouts/content-no-sidebar" %}
    {% case "comunidadDirectLink" %}
        {% include "_layouts/direct-link" %}
    {% default %}
        {% include"_layouts/3-base" %}
{% endswitch %}


